#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>


int main() {
    float num;
    scanf("%f", &num);
    printf("%f\n", num);
    printf("%.4f\n", num);
    printf("%.2f\n", num);
    printf("%.0f\n", round(num));

    return 0;
}
